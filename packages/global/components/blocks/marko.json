{
  "taglib-imports": [
    "./load-more/marko.json"
  ],
  "<global-most-popular-list-block>": {
    "template": "./most-popular-list.marko"
  },
  "<global-sponsors-block>": {
    "template": "./sponsors.marko",
    "@alias": "string"
  },
  "<global-external-api-block>": {
    "template": "./external-api.marko",
    "@api-url": "string",
    "@resolved-to-nodes-converter": "object",
    "@name": "string",
    "@description": "string",
    "@with-sponsor": "boolean",
    "@link-header": "boolean",
    "@button": "object",
    "@native-x": "object"
  },
  "<global-collapsible-sidebar-block>": {
    "template": "./collapsible-sidebar.marko",
    "@item": "object",
    "@type": "string"
  },
  "<global-icle-top-posts-block>": {
    "template": "./icle-top-posts.marko",
    "@api-url": "string"
  }
}
